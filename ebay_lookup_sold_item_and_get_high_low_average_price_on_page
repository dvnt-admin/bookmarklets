javascript:(function(){const n=prompt("Enter the name of the item");if(n){const e=encodeURIComponent(n.replace(/ /g,'+').replace(/"/g,'%22')),t=`https://www.ebay.com/sch/i.html?_nkw=${e}&LH_TitleDesc=1&_dmd=1&_sop=1&LH_Complete=1&_ipg=240&LH_Sold=1`,o=window.open(t,"_blank");o.onload=function(){const e=o.document.querySelectorAll('div.s-item__details:nth-of-type(n+3) div.s-item__detail:nth-of-type(1) span.POSITIVE');if(e.length>0){const t=Array.from(e).map(e=>parseFloat(e.textContent.replace('$',''))),n=Math.max(...t).toFixed(2),a=Math.min(...t).toFixed(2),r=(t.reduce((e,t)=>e+t,0)/t.length).toFixed(2),i=o.document.createElement('div');i.style.cssText='position:fixed;top:0;left:0;background-color:#fff;padding:10px;z-index:9999;',i.innerHTML=`Highest%20Price:%20$${n}<br>Lowest%20Price:%20$${a}<br>Average%20Price:%20$${r}`,o.document.body.appendChild(i)}else%20alert('No%20sold%20prices%20found%20on%20this%20eBay%20page.')}}})();
